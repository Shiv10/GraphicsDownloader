{
    "name": "Graphics Downloader",
    "description": "A chrome extension to download png, svg, jpeg, jpg files from an html doc.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "downloads"
    ]
}